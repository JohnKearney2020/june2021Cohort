<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Partials use a dash -->
    <%- include('partials/head') %> 
  </head>
  <body>
    <%- include('partials/nav') %> 
    <% specials.forEach((special) => { %>
      <h2><%= special.name %></h2>
      <h3><%= special.price %></h3>
      <p><%= special.description %> </p>
      <a href="/menu/<%= special.id %>"> View Item</a>
    <% }) %>
  </body>
</html>